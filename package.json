{
  "name": "tiny-tripper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "dotenvx run --convention=nextjs -- next dev",
    "dev:backend": "dotenvx run --convention=nextjs -- convex dev",
    "predev": "dotenvx run --convention=nextjs -- convex dev --until-success && convex dashboard",
    "build": "dotenvx run --convention=nextjs -- next build",
    "start": "dotenvx run --convention=nextjs -- next start",
    "lint": "dotenvx run --convention=nextjs -- next lint",
    "upload-embeddings": "dotenvx run --convention=nextjs -- tsx scripts/upload-embeddings-batch.ts",
    "merge-embeddings": "dotenvx run --convention=nextjs -- tsx scripts/merge-embeddings.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.60.0",
    "@clerk/nextjs": "^6.37.1",
    "@convex-dev/geospatial": "^0.1.9",
    "@convex-dev/workflow": "^0.2.7",
    "@dotenvx/dotenvx": "^1.52.0",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@mendable/firecrawl-js": "^4.12.0",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@radix-ui/themes": "^3.3.0",
    "@t3-oss/env-core": "^0.13.10",
    "@t3-oss/env-nextjs": "^0.13.10",
    "convex": "^1.31.7",
    "jiti": "^2.6.1",
    "lucide-react": "^0.542.0",
    "next": "15.2.8",
    "openai": "^5.23.2",
    "posthog-js": "^1.336.4",
    "posthog-node": "^5.24.7",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20.19.30",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.2.3",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
